                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$abduction_adduction_C18)), which(colnames(wod_rs[[6]]) == "abduction_adduction_C18")]),
  
  oROM_wod_z = c(wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C8)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C8")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C9)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C9")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C10)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C10")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C11)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C11")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C12)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C12")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C13)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C13")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C14)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C14")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C15)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C15")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C16)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C16")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C17)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C17")],
                 wod_rs[[6]][which.max(abs(wod_rs$CBP_wod_u_real_rs$protraction_retraction_C18)), which(colnames(wod_rs[[6]]) == "protraction_retraction_C18")])
  
)


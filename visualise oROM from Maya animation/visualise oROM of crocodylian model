                 wod_r[[2]][which.max(abs(wod_r$AB_wod_u_real_r$protraction_retraction_C15)), which(colnames(wod_r[[2]]) == "protraction_retraction_C15")],
                 wod_r[[2]][which.max(abs(wod_r$AB_wod_u_real_r$protraction_retraction_C16)), which(colnames(wod_r[[2]]) == "protraction_retraction_C16")],
                 wod_r[[2]][which.max(abs(wod_r$AB_wod_u_real_r$protraction_retraction_C17)), which(colnames(wod_r[[2]]) == "protraction_retraction_C17")],
                 wod_r[[2]][which.max(abs(wod_r$AB_wod_u_real_r$protraction_retraction_C18)), which(colnames(wod_r[[2]]) == "protraction_retraction_C18")]))



z_AJ_bar <- ggplot() +
  geom_bar(data = vij_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z), stat = "identity", fill = "#E7B10A", alpha = .3) +
  geom_text(data = vij_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z, label = oROM_max_z), vjust = ifelse(vij_r_AJ_bar_data$oROM_max_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = vij_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z), stat = "identity", fill = "#E7B10A", alpha = .6) +
  geom_text(data = vij_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z, label = oROM_wod_z), vjust = ifelse(vij_r_AJ_bar_data$oROM_wod_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = vij_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#E7B10A", alpha = .8) +
  geom_text(data = vij_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(vij_r_AJ_bar_data$oROM_wd_z > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))

z_CBP_bar <- ggplot() +
  geom_bar(data = vij_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z), stat = "identity", fill = "#041E42", alpha = .3) +
  geom_text(data = vij_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z, label = oROM_max_z), vjust = ifelse(vij_r_CBP_bar_data$oROM_max_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = vij_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z), stat = "identity", fill = "#041E42", alpha = .6) +
  geom_text(data = vij_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z, label = oROM_wod_z), vjust = ifelse(vij_r_CBP_bar_data$oROM_wod_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = vij_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#041E42", alpha = .8) +
  geom_text(data = vij_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(vij_r_CBP_bar_data$oROM_wd_z > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))

p <- ggarrange(x_AB_bar, x_AJ_bar, x_CBP_bar,
               y_AB_bar, y_AJ_bar, y_CBP_bar,
               z_AB_bar, z_AJ_bar, z_CBP_bar,
               ncol = 3, nrow = 3,
               labels = c("A", "B", "C",
                          "D", "E", "F",
                          "G", "H", "I"),
               common.legend = TRUE, legend = "bottom")

ggsave(plot = p, dpi = 600, bg = "white", 
       filename = "croc_ventral_intracos_r_bar.png", 
       device = "png", width = 22, heigh = 28, units = "cm")



# generate indidivdual plot for intervertebral dorsointracostal ventrointracostal joints
# generate IV plots
for(i in 2:13){
  temp_um <- data.frame(
    IV_jnts[[2]][c(1, i)], IV_jnts[[4]][i], IV_jnts[[6]][i])
  colnames(temp_um) <- c("time", "flexex_max", "flexex_wd", "flexex_wod")
  temp_um$time <- (temp_um$time/4)
  
  temp_ca <- data.frame(
    IV_jnts[[1]][c(1, i)], IV_jnts[[3]][i], IV_jnts[[5]][i])
  colnames(temp_ca) <- c("time", "flexex_max", "flexex_wd", "flexex_wod")
  temp_ca$time <- (temp_ca$time/4)
  
  bar_data <- data.frame(
    jcs = c("Flexion extension"),
    oROM_max = temp_um[which.max(abs(temp_um$flexex_max)), which(colnames(temp_um) == "flexex_max")],
    oROM_wd = temp_um[which.max(abs(temp_um$flexex_wd)), which(colnames(temp_um) == "flexex_wd")],
    oROM_wod = temp_um[which.max(abs(temp_um$flexex_wod)), which(colnames(temp_um) == "flexex_wod")]
  )
  
  x_max <- ggplot() +
    geom_line(data = temp_um, aes(x = time, y = flexex_max), colour = "#FF4C00", linewidth = 1.5, alpha = .4) +
    geom_line(data = temp_ca, aes(x = time, y = flexex_max), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") +
    geom_hline(yintercept = 0, linetype = "solid", color = "black", linewidth = 1) + 
    scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
    xlab("Percentage of ventilatory cycle") +
    ylab("Joint rotations (deg)") +
    theme_classic() +
    theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
          axis.text = element_text(size = 5),
          axis.title = element_text(size = 7))
  
  x_wd <- ggplot() +
    geom_line(data = temp_um, aes(x = time, y = flexex_wd), colour = "#FF4C00", linewidth = 1.5, alpha = .4) +
    geom_line(data = temp_ca, aes(x = time, y = flexex_wd), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") +
    geom_hline(yintercept = 0, linetype = "solid", color = "black", linewidth = 1) + 
    scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
    xlab("Percentage of ventilatory cycle") +
    ylab("Joint rotations (deg)") +
    theme_classic() +
    theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
          axis.text = element_text(size = 5),
          axis.title = element_text(size = 7)) 
  
  x_wod <- ggplot() +
    geom_line(data = temp_um, aes(x = time, y = flexex_wod), colour = "#FF4C00", linewidth = 1.5, alpha = .4) +
    geom_line(data = temp_ca, aes(x = time, y = flexex_wod), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") +
    geom_hline(yintercept = 0, linetype = "solid", color = "black", linewidth = 1) + 
    scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
    xlab("Percentage of ventilatory cycle") +
    ylab("Joint rotations (deg)") +
    theme_classic() +
    theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
          axis.text = element_text(size = 5),
          axis.title = element_text(size = 7)) 
  
  x_bar <- ggplot() +
    geom_bar(data = bar_data, aes(x = jcs, y = oROM_max, fill = jcs), stat = "identity", alpha = .3) + 
    geom_text(data = bar_data, aes(x = jcs, y = oROM_max, label = oROM_max), vjust = ifelse(bar_data$oROM_max > 0, 1, -1), size = 2, colour = "black") +
    geom_bar(data = bar_data, aes(x = jcs, y = oROM_wd, fill = jcs), stat = "identity", alpha = .6) +
    geom_text(data = bar_data, aes(x = jcs, y = oROM_wd, label = oROM_wd), vjust = ifelse(bar_data$oROM_wd > 0, 1, -0.5), size = 2, colour = "black") +
    geom_bar(data = bar_data, aes(x = jcs, y = oROM_wod, fill = jcs), stat = "identity", alpha = .8) +
    geom_text(data = bar_data, aes(x = jcs, y = oROM_wod, label = oROM_wod), vjust = ifelse(bar_data$oROM_wod > 0, 1, -0.5), size = 2, colour = "black") +
    scale_fill_manual(name = "Flexion extension", values = c("#FF4C00")) +
    geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
    xlab("Joint coordinate systems (JCSs)") +
    ylab("Maximal joint rotations (deg)") +
    theme_classic() +
    theme(axis.line.x = element_blank(),
          axis.ticks.x = element_blank(),
          axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
          axis.text = element_text(size = 5),
          axis.title = element_text(size = 7),
          legend.title = element_blank())
  
  p <- ggarrange(x_max, x_wd, x_wod, x_bar,
                 ncol = 4, nrow = 1,
                 labels = c("A", "B", "C"),
                 common.legend = TRUE, legend = "bottom")
  
  ggsave(plot = p, dpi = 600, bg = "white", 
         filename = paste0("croc_", IV_names[(i-1)], ".png"), 
         device = "png", width = 22, heigh = 14, units = "cm")
}

# arrange the rib joint data for plotting
plot_max <- list(max_vrs, max_rs, max_r)
plot_wd <- list(wd_vrs, wd_rs, wd_r)
plot_wod <- list(wod_vrs, wd_rs, wod_r)

# generate rib joint plots
for(d in 1:length(plot_max)){
  for(i in seq(from = 2, to = 74, by = 3)){
    temp_um <- data.frame(
      plot_max[[d]][[2]][c(1, i:(i + 2))], plot_wd[[d]][[2]][c(i:(i + 2))], plot_wod[[d]][[2]][c(i:(i + 2))],
      plot_max[[d]][[4]][c(i:(i + 2))], plot_wd[[d]][[4]][c(i:(i + 2))], plot_wod[[d]][[4]][c(i:(i + 2))],
      plot_max[[d]][[6]][c(i:(i + 2))], plot_wd[[d]][[6]][c(i:(i + 2))], plot_wod[[d]][[6]][c(i:(i + 2))])
    colnames(temp_um) <- c("time", "ax_AB_max", "abad_AB_max", "prore_AB_max", 
                           "ax_AB_wd", "abad_AB_wd", "prore_AB_wd",
                           "ax_AB_wod", "abad_AB_wod", "prore_AB_wod",
                           "ax_AJ_max", "abad_AJ_max", "prore_AJ_max",
                           "ax_AJ_wd", "abad_AJ_wd", "prore_AJ_wd",
                           "ax_AJ_wod", "abad_AJ_wod", "prore_AJ_wod",
                           "pum_CBP_max", "cali_CBP_max", "buck_CBP_max",
                           "pum_CBP_wd", "cali_CBP_wd", "buck_CBP_wd",
                           "pum_CBP_wod", "cali_CBP_wod", "buck_CBP_wod")
    temp_um$time <- (temp_um$time/6)
    
    
    temp_ca <- data.frame(
      plot_max[[d]][[1]][c(1, i:(i + 2))], plot_wd[[d]][[1]][c(i:(i + 2))], plot_wod[[d]][[1]][c(i:(i + 2))],
      plot_max[[d]][[3]][c(i:(i + 2))], plot_wd[[d]][[3]][c(i:(i + 2))], plot_wod[[d]][[3]][c(i:(i + 2))],
      plot_max[[d]][[5]][c(i:(i + 2))], plot_wd[[d]][[5]][c(i:(i + 2))], plot_wod[[d]][[5]][c(i:(i + 2))])
    colnames(temp_ca) <- c("time", "ax_AB_max", "abad_AB_max", "prore_AB_max", 
                           "ax_AB_wd", "abad_AB_wd", "prore_AB_wd",
                           "ax_AB_wod", "abad_AB_wod", "prore_AB_wod",
                           "ax_AJ_max", "abad_AJ_max", "prore_AJ_max",
                           "ax_AJ_wd", "abad_AJ_wd", "prore_AJ_wd",
                           "ax_AJ_wod", "abad_AJ_wod", "prore_AJ_wod",
                           "pum_CBP_max", "cali_CBP_max", "buck_CBP_max",
                           "pum_CBP_wd", "cali_CBP_wd", "buck_CBP_wd",
                           "pum_CBP_wod", "cali_CBP_wod", "buck_CBP_wod")
    temp_ca$time <- (temp_ca$time/6)
    
    #only the uniform data is used to bar plots as max values are the same
    bar_data <- data.frame(
      jcs = c("AB", "AJ", "CBP"),
      oROM_max_x = c(temp_um[which.max(abs(temp_um$ax_AB_max)), which(colnames(temp_um) == "ax_AB_max")], 
                     temp_um[which.max(abs(temp_um$ax_AJ_max)), which(colnames(temp_um) == "ax_AJ_max")], 
                     temp_um[which.max(abs(temp_um$pum_CBP_max)), which(colnames(temp_um) == "pum_CBP_max")]),
      
      oROM_max_y = c(temp_um[which.max(abs(temp_um$abad_AB_max)), which(colnames(temp_um) == "abad_AB_max")], 
                     temp_um[which.max(abs(temp_um$abad_AJ_max)), which(colnames(temp_um) == "abad_AJ_max")], 
                     temp_um[which.max(abs(temp_um$cali_CBP_max)), which(colnames(temp_um) == "cali_CBP_max")]),
      
      oROM_max_z = c(temp_um[which.max(abs(temp_um$prore_AB_max)), which(colnames(temp_um) == "prore_AB_max")], 
                     temp_um[which.max(abs(temp_um$prore_AJ_max)), which(colnames(temp_um) == "prore_AJ_max")], 
                     temp_um[which.max(abs(temp_um$buck_CBP_max)), which(colnames(temp_um) == "buck_CBP_max")]),
      
      oROM_wd_x = c(temp_um[which.max(abs(temp_um$ax_AB_wd)), which(colnames(temp_um) == "ax_AB_wd")], 
                    temp_um[which.max(abs(temp_um$ax_AJ_wd)), which(colnames(temp_um) == "ax_AJ_wd")], 
                    temp_um[which.max(abs(temp_um$pum_CBP_wd)), which(colnames(temp_um) == "pum_CBP_wd")]),
      
      oROM_wd_y = c(temp_um[which.max(abs(temp_um$abad_AB_wd)), which(colnames(temp_um) == "abad_AB_wd")], 
                    temp_um[which.max(abs(temp_um$abad_AJ_wd)), which(colnames(temp_um) == "abad_AJ_wd")], 
                    temp_um[which.max(abs(temp_um$cali_CBP_wd)), which(colnames(temp_um) == "cali_CBP_wd")]),
      
      oROM_wd_z = c(temp_um[which.max(abs(temp_um$prore_AB_wd)), which(colnames(temp_um) == "prore_AB_wd")], 
                    temp_um[which.max(abs(temp_um$prore_AJ_wd)), which(colnames(temp_um) == "prore_AJ_wd")], 
                    temp_um[which.max(abs(temp_um$buck_CBP_wd)), which(colnames(temp_um) == "buck_CBP_wd")]),
      
      oROM_wod_x = c(temp_um[which.max(abs(temp_um$ax_AB_wod)), which(colnames(temp_um) == "ax_AB_wod")], 
                     temp_um[which.max(abs(temp_um$ax_AJ_wod)), which(colnames(temp_um) == "ax_AJ_wod")], 
                     temp_um[which.max(abs(temp_um$pum_CBP_wod)), which(colnames(temp_um) == "pum_CBP_wod")]),
      
      oROM_wod_y = c(temp_um[which.max(abs(temp_um$abad_AB_wod)), which(colnames(temp_um) == "abad_AB_wod")], 
                     temp_um[which.max(abs(temp_um$abad_AJ_wod)), which(colnames(temp_um) == "abad_AJ_wod")], 
                     temp_um[which.max(abs(temp_um$cali_CBP_wod)), which(colnames(temp_um) == "cali_CBP_wod")]),
      
      oROM_wod_z = c(temp_um[which.max(abs(temp_um$prore_AB_wod)), which(colnames(temp_um) == "prore_AB_wod")], 
                     temp_um[which.max(abs(temp_um$prore_AJ_wod)), which(colnames(temp_um) == "prore_AJ_wod")], 
                     temp_um[which.max(abs(temp_um$buck_CBP_wod)), which(colnames(temp_um) == "buck_CBP_wod")])
    )
    
    x_max <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = ax_AB_max), colour = "#FF4C00", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = ax_AB_max), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = ax_AJ_max), colour = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = ax_AJ_max), colour = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = pum_CBP_max), colour = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = pum_CBP_max), colour = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", linewidth = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    x_wd <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = ax_AB_wd), colour = "#FF4C00", linewidth = 1.5, alpha = .4) + 
      geom_line(data = temp_ca, aes(x = time, y = ax_AB_wd), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") + 
      geom_line(data = temp_um, aes(x = time, y = ax_AJ_wd), colour = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = ax_AJ_wd), colour = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = pum_CBP_wd), colour = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = pum_CBP_wd), colour = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", linewidth = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    x_wod <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = ax_AB_wod), colour = "#FF4C00", linewidth = 1.5, alpha = .4) + 
      geom_line(data = temp_ca, aes(x = time, y = ax_AB_wod), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") + 
      geom_line(data = temp_um, aes(x = time, y = ax_AJ_wod), colour = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = ax_AJ_wod), colour = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = pum_CBP_wod), colour = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = pum_CBP_wod), colour = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", linewidth = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    x_bar <- ggplot() +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_max_x, fill = jcs), stat = "identity", alpha = .3) + 
      geom_text(data = bar_data, aes(x = jcs, y = oROM_max_x, label = oROM_max_x), vjust = ifelse(bar_data$oROM_max_x > 0, 1, -1), size = 2, colour = "black") +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_wod_x, fill = jcs), stat = "identity", alpha = .6) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_wod_x, label = oROM_wod_x), vjust = ifelse(bar_data$oROM_wod_x > 0, 1, -0.5), size = 2, colour = "black") +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_wd_x, fill = jcs), stat = "identity", alpha = .8) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_wd_x, label = oROM_wd_x), vjust = ifelse(bar_data$oROM_wd_x > 0, 1, -0.5), size = 2, colour = "black") +
      scale_fill_manual(name = "Types of JCS", values = c("#FF4C00", "#E7B10A", "#041E42")) +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      xlab("Joint coordinate systems (JCSs)") +
      ylab("Maximal joint rotations (deg)") +
      theme_classic() +
      theme(axis.line.x = element_blank(),
            axis.ticks.x = element_blank(),
            axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    y_max <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = abad_AB_max), colour = "#FF4C00", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = abad_AB_max), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = abad_AJ_max), colour = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = abad_AJ_max), colour = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = cali_CBP_max), colour = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = cali_CBP_max), colour = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    y_wd <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = abad_AB_wd), colour = "#FF4C00", linewidth = 1.5, alpha = .4) + 
      geom_line(data = temp_ca, aes(x = time, y = abad_AB_wd), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") + 
      geom_line(data = temp_um, aes(x = time, y = abad_AJ_wd), colour = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = abad_AJ_wd), colour = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = cali_CBP_wd), colour = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = cali_CBP_wd), colour = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    y_wod <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = abad_AB_wod), colour = "#FF4C00", linewidth = 1.5, alpha = .4) + 
      geom_line(data = temp_ca, aes(x = time, y = abad_AB_wod), colour = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") + 
      geom_line(data = temp_um, aes(x = time, y = abad_AJ_wod), colour = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = abad_AJ_wod), colour = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = cali_CBP_wod), colour = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = cali_CBP_wod), colour = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    y_bar <- ggplot() +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_max_y, fill = jcs), stat = "identity", alpha = .3) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_max_y, label = oROM_max_y), vjust = ifelse(bar_data$oROM_max_y > 0, 1, -1), size = 2, colour = "black") +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_wod_y, fill = jcs), stat = "identity", alpha = .6) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_wod_y, label = oROM_wod_y), vjust = ifelse(bar_data$oROM_wod_y > 0, 1, -0.5), size = 2, colour = "black") +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_wd_y, fill = jcs), stat = "identity", alpha = .8) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_wd_y, label = oROM_wd_y), vjust = ifelse(bar_data$oROM_wd_y > 0, 1, -0.5), size = 2, colour = "black") +
      scale_fill_manual(name = "Types of JCS", values = c("#FF4C00", "#E7B10A", "#041E42")) +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      xlab("Joint coordinate systems (JCSs)") +
      ylab("Maximal joint rotations (deg)") +
      theme_classic() +
      theme(axis.line.x = element_blank(),
            axis.ticks.x = element_blank(),
            axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    z_max <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = prore_AB_max), color = "#FF4C00", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = prore_AB_max), color = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = prore_AJ_max), color = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = prore_AJ_max), color = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = buck_CBP_max), color = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = buck_CBP_max), color = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    z_wd <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = prore_AB_wd), color = "#FF4C00", linewidth = 1.5, alpha = .4) + 
      geom_line(data = temp_ca, aes(x = time, y = prore_AB_wd), color = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") + 
      geom_line(data = temp_um, aes(x = time, y = prore_AJ_wd), color = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = prore_AJ_wd), color = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = buck_CBP_wd), color = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = buck_CBP_wd), color = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    z_wod <- ggplot() +
      geom_line(data = temp_um, aes(x = time, y = prore_AB_wod), color = "#FF4C00", linewidth = 1.5, alpha = .4) + 
      geom_line(data = temp_ca, aes(x = time, y = prore_AB_wod), color = "#FF4C00", linewidth = 1.5, alpha = .8, linetype = "twodash") + 
      geom_line(data = temp_um, aes(x = time, y = prore_AJ_wod), color = "#E7B10A", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = prore_AJ_wod), color = "#E7B10A", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_line(data = temp_um, aes(x = time, y = buck_CBP_wod), color = "#041E42", linewidth = 1.5, alpha = .4) +
      geom_line(data = temp_ca, aes(x = time, y = buck_CBP_wod), color = "#041E42", linewidth = 1.5, alpha = .8, linetype = "twodash") +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      scale_x_continuous(expand = c(0, 0), limits = c(0, NA)) +
      xlab("Percentage of ventilatory cycle") +
      ylab("Joint rotations (deg)") +
      theme_classic() +
      theme(axis.line = element_line(colour = "black", linewidth = 0.5, linetype = "solid"), 
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    z_bar <- ggplot() +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_max_z, fill = jcs), stat = "identity", alpha = .3) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_max_z, label = oROM_max_z), vjust = ifelse(bar_data$oROM_max_z > 0, 1, -1), size = 2, colour = "black") +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_wod_z, fill = jcs), stat = "identity", alpha = .6) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_wod_z, label = oROM_wod_z), vjust = ifelse(bar_data$oROM_wod_z > 0, 1, -0.5), size = 2, colour = "black") +
      geom_bar(data = bar_data, aes(x = jcs, y = oROM_wd_z, fill = jcs), stat = "identity", alpha = .8) +
      geom_text(data = bar_data, aes(x = jcs, y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(bar_data$oROM_wd_z > 0, 1, -0.5), size = 2, colour = "black") +
      scale_fill_manual(name = "Types of JCS", values = c("#FF4C00", "#E7B10A", "#041E42")) +
      geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
      xlab("Joint coordinate systems (JCSs)") +
      ylab("Maximal joint rotations (deg)") +
      theme_classic() +
      theme(axis.line.x = element_blank(),
            axis.ticks.x = element_blank(),
            axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
            axis.text = element_text(size = 10),
            axis.title = element_text(size = 12))
    
    p <- ggarrange(x_max, x_wd, x_wod, x_bar,
                   y_max, y_wd, y_wod, y_bar,
                   z_max, z_wd, z_wod, z_bar,
                   ncol = 4, nrow = 3,
                   labels = c("A", "B", "C",
                              "D", "E", "F",
                              "G", "H", "I"),
                   common.legend = TRUE, legend = "bottom")
    
    ggsave(plot = p, dpi = 600, bg = "white", 
           filename = paste0("croc_", Rib_names[((i+1)/3)], seg_names[[d]][[1]][((i+1)/3)], ".png"), 
           device = "png", width = 22, heigh = 28, units = "cm")
  }
}

# combine max values of oROM for supplementary information and for SIMM chapter
#AB setup
oROM_AB_vrs <- data.frame() %>%
  rbind(cj_vrs_AB_bar_data, dij_vrs_AB_bar_data, vij_vrs_AB_bar_data)
oROM_AB_vrs <-cbind(oROM_AB_vrs, "JCS" = "AB", "MT" = "vrs")

oROM_AB_rs <- data.frame() %>%
  rbind(cj_rs_AB_bar_data, dij_rs_AB_bar_data, vij_rs_AB_bar_data)
oROM_AB_rs <-cbind(oROM_AB_rs, "JCS" = "AB", "MT" = "rs")

oROM_AB_r <- data.frame() %>%
  rbind(cj_r_AB_bar_data, dij_r_AB_bar_data, vij_r_AB_bar_data)
oROM_AB_r <-cbind(oROM_AB_r, "JCS" = "AB", "MT" = "r")

#AJ setup
oROM_AJ_vrs <- data.frame() %>%
  rbind(cj_vrs_AJ_bar_data, dij_vrs_AJ_bar_data, vij_vrs_AJ_bar_data)
oROM_AJ_vrs <-cbind(oROM_AJ_vrs, "JCS" = "AJ", "MT" = "vrs")

oROM_AJ_rs <- data.frame() %>%
  rbind(cj_rs_AJ_bar_data, dij_rs_AJ_bar_data, vij_rs_AJ_bar_data)
oROM_AJ_rs <-cbind(oROM_AJ_rs, "JCS" = "AJ", "MT" = "rs")

oROM_AJ_r <- data.frame() %>%
  rbind(cj_r_AJ_bar_data, dij_r_AJ_bar_data, vij_r_AJ_bar_data)
oROM_AJ_r <-cbind(oROM_AJ_r, "JCS" = "AJ", "MT" = "r")

#CBP setup
oROM_CBP_vrs <- data.frame() %>%
  rbind(cj_vrs_CBP_bar_data, dij_vrs_CBP_bar_data, vij_vrs_CBP_bar_data)
oROM_CBP_vrs <-cbind(oROM_CBP_vrs, "JCS" = "CBP", "MT" = "vrs")

oROM_CBP_rs <- data.frame() %>%
  rbind(cj_rs_CBP_bar_data, dij_rs_CBP_bar_data, vij_rs_CBP_bar_data)
oROM_CBP_rs <-cbind(oROM_CBP_rs, "JCS" = "CBP", "MT" = "rs")

oROM_CBP_r <- data.frame() %>%
  rbind(cj_r_CBP_bar_data, dij_r_CBP_bar_data, vij_r_CBP_bar_data)
oROM_CBP_r <-cbind(oROM_CBP_r, "JCS" = "CBP", "MT" = "r")

#combine all oROM into one table
oROM <- rbind(oROM_AB_vrs, oROM_AJ_vrs, oROM_CBP_vrs,
              oROM_AB_rs, oROM_AJ_rs, oROM_CBP_rs,
              oROM_AB_r, oROM_AJ_r, oROM_CBP_r)
oROM <- oROM[order(oROM$jnt, oROM$MT),]

#export csv file
write.csv(oROM, "oROM_croc.csv", row.names = FALSE)
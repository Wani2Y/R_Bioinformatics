  oROM_max_x = c(max_r[[6]][which.max(abs(max_r$CBP_u_max_r$axial_rotation_SR10)), which(colnames(max_r[[6]]) == "axial_rotation_SR10")],
                 max_r[[6]][which.max(abs(max_r$CBP_u_max_r$axial_rotation_SR11)), which(colnames(max_r[[6]]) == "axial_rotation_SR11")],

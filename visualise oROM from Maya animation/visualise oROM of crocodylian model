# make bar plots
# vrs version
# costal joints
x_AB_bar <- ggplot() +
  geom_bar(data = cj_vrs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_x), stat = "identity", fill = "#FF4C00", alpha = .3) +
  geom_text(data = cj_vrs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_x, label = oROM_max_x), vjust = ifelse(cj_vrs_AB_bar_data$oROM_max_x > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_vrs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_x), stat = "identity", fill = "#FF4C00", alpha = .6) +
  geom_text(data = cj_vrs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_x, label = oROM_wod_x), vjust = ifelse(cj_vrs_AB_bar_data$oROM_wod_x > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_vrs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x), stat = "identity", fill = "#FF4C00", alpha = .8) +
  geom_text(data = cj_vrs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x, label = oROM_wd_x), vjust = ifelse(cj_vrs_AB_bar_data$oROM_wd_x > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))

x_AJ_bar <- ggplot() +
  geom_bar(data = cj_vrs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_x), stat = "identity", fill = "#E7B10A", alpha = .3) +
  geom_text(data = cj_vrs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_x, label = oROM_max_x), vjust = ifelse(cj_vrs_AJ_bar_data$oROM_max_x > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_vrs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_x), stat = "identity", fill = "#E7B10A", alpha = .6) +
  geom_text(data = cj_vrs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_x, label = oROM_wod_x), vjust = ifelse(cj_vrs_AJ_bar_data$oROM_wod_x > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_vrs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x), stat = "identity", fill = "#E7B10A", alpha = .8) +
  geom_text(data = cj_vrs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x, label = oROM_wd_x), vjust = ifelse(cj_vrs_AJ_bar_data$oROM_wd_x > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))


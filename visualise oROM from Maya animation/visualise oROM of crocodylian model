  geom_bar(data = dij_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_y), stat = "identity", fill = "#E7B10A", alpha = .3) +
  geom_text(data = dij_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_y, label = oROM_max_y), vjust = ifelse(dij_rs_AJ_bar_data$oROM_max_y > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = dij_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_y), stat = "identity", fill = "#E7B10A", alpha = .6) +
  geom_text(data = dij_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_y, label = oROM_wod_y), vjust = ifelse(dij_rs_AJ_bar_data$oROM_wod_y > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = dij_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_y), stat = "identity", fill = "#E7B10A", alpha = .8) +
  geom_text(data = dij_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_y, label = oROM_wd_y), vjust = ifelse(dij_rs_AJ_bar_data$oROM_wd_y > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))


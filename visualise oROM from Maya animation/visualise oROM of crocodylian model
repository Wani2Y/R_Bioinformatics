  geom_bar(data = cj_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#E7B10A", alpha = .8) +
  geom_text(data = cj_rs_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(cj_rs_AJ_bar_data$oROM_wd_z > 0, 1, -1), size = 1, colour = "black") +

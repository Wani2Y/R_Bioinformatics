                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$protraction_retraction_IR13)), which(colnames(max_rs[[2]]) == "protraction_retraction_IR13")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$protraction_retraction_IR14)), which(colnames(max_rs[[2]]) == "protraction_retraction_IR14")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$protraction_retraction_IR15)), which(colnames(max_rs[[2]]) == "protraction_retraction_IR15")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$protraction_retraction_IR16)), which(colnames(max_rs[[2]]) == "protraction_retraction_IR16")]),
  
  oROM_wd_x = c(wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$axial_rotation_IR10)), which(colnames(wd_rs[[2]]) == "axial_rotation_IR10")],

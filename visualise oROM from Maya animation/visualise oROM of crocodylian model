  geom_text(data = cj_vrs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z, label = oROM_wod_z), vjust = ifelse(cj_vrs_CBP_bar_data$oROM_wod_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_vrs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#041E42", alpha = .8) +

# vrs version
dij_vrs_AB_bar_data <- data.frame(
  jnt = c("DIR10", "DIR11", "DIR12", "DIR13", "DIR14", "DIR15", "DIR16"),
  oROM_max_x = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR10)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR11)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR11")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR12)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR12")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR13)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR13")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR14)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR14")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR15)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR15")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_IR16)), which(colnames(max_vrs[[2]]) == "axial_rotation_IR16")]),
  
  oROM_max_y = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_IR10)), which(colnames(max_vrs[[2]]) == "abduction_adduction_IR10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_IR11)), which(colnames(max_vrs[[2]]) == "abduction_adduction_IR11")],

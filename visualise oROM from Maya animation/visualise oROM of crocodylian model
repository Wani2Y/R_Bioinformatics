  jnt = c("VIR10", "VIR11", "VIR12", "VIR13", "VIR14", "VIR15", "VIR16"),
  oROM_max_x = c(max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR10)), which(colnames(max_rs[[2]]) == "axial_rotation_SR10")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR11)), which(colnames(max_rs[[2]]) == "axial_rotation_SR11")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR12)), which(colnames(max_rs[[2]]) == "axial_rotation_SR12")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR13)), which(colnames(max_rs[[2]]) == "axial_rotation_SR13")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR14)), which(colnames(max_rs[[2]]) == "axial_rotation_SR14")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR15)), which(colnames(max_rs[[2]]) == "axial_rotation_SR15")],
                 max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$axial_rotation_SR16)), which(colnames(max_rs[[2]]) == "axial_rotation_SR16")]),
  
  oROM_max_y = c(max_rs[[2]][which.max(abs(max_rs$AB_u_max_rs$abduction_adduction_SR10)), which(colnames(max_rs[[2]]) == "abduction_adduction_SR10")],

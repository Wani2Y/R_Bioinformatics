                 max_rs[[6]][which.max(abs(max_rs$CBP_u_max_rs$protraction_retraction_IR16)), which(colnames(max_rs[[6]]) == "protraction_retraction_IR16")]),
  
  oROM_wd_x = c(wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR10)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR10")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR11)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR11")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR12)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR12")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR13)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR13")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR14)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR14")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR15)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR15")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_IR16)), which(colnames(wd_rs[[6]]) == "axial_rotation_IR16")]),
  
  oROM_wd_y = c(wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$abduction_adduction_IR10)), which(colnames(wd_rs[[6]]) == "abduction_adduction_IR10")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$abduction_adduction_IR11)), which(colnames(wd_rs[[6]]) == "abduction_adduction_IR11")],

# title: "**Generating oROM plot for crocodylian animations from Maya**"
# project for: "oROM estimationg using Maya"
# author: "Wani2Y"
# first created: "28/02/2023"
# last modified: "11/03/2023"

# load the necessary libraries
library(tidyverse)
library(data.table)
library(ggplot2)
library(ggpubr)
library(gridExtra)
library(stringi)

## read in the oROM from csv files
# read the filenames of all csv files without extensions
filenames <- tools::file_path_sans_ext(
  list.files(path ="./crocodylian/",
             pattern ="*.csv")) 

# read in all csv files from subfolder
big_table <- list()
for(i in filenames){
  filepath <- file.path("./crocodylian/",paste(i,".csv",sep =""))
  big_table[[i]] <- read.csv(filepath)
}

# segregate tables from big_table
# get plot data for IV
IV_jnts <- list()
for(i in 1: length(big_table)){
  if (str_detect(names(big_table[i]), "_vrs")){
    if(str_detect(names(big_table[i]), "AB")){
      IV_jnts = append(IV_jnts, big_table[i])
    }
  }
}

for (j in 1:length(IV_jnts)){
  IV_jnts[[j]] = IV_jnts[[j]][c(1, (5:16))]
}

# get plot data for max oROM
max_oROM <- list()
for(i in 1: length(big_table)){
  if (str_detect(names(big_table[i]), "_max_")){
    max_oROM = append(max_oROM, big_table[i])
  }
}

# get VRS
max_vrs <- list()
for(i in 1: length(max_oROM)){
  if (str_detect(names(max_oROM[i]), "_vrs")){
    max_vrs = append(max_vrs, max_oROM[i])
  }
}

for (j in 1:length(max_vrs)){
  max_vrs[[j]] = max_vrs[[j]][-c(2:16)]
}

# get RS
max_rs <- list()
for(i in 1: length(max_oROM)){
  if (str_detect(names(max_oROM[i]), "_rs")){
    max_rs = append(max_rs, max_oROM[i])
  }
}

for (j in 1:length(max_rs)){
  max_rs[[j]] = max_rs[[j]][-c(2:4)]
}

# get R
max_r <- list()
for(i in seq(from = 1, to =  length(max_oROM), by = 3)){
    max_r = append(max_r, max_oROM[i])
}


# get plot data for wd oROM
wd_oROM <- list()
for(i in 1: length(big_table)){
  if (str_detect(names(big_table[i]), "_wd_")){
    wd_oROM = append(wd_oROM, big_table[i])
  }
}

# get VRS
wd_vrs <- list()
for(i in 1: length(wd_oROM)){
  if (str_detect(names(wd_oROM[i]), "_vrs")){
    wd_vrs = append(wd_vrs, wd_oROM[i])
  }
}

for (j in 1:length(wd_vrs)){
  wd_vrs[[j]] = wd_vrs[[j]][-c(2:16)]
}

# get RS
wd_rs <- list()
for(i in 1: length(wd_oROM)){
  if (str_detect(names(wd_oROM[i]), "_rs")){
    wd_rs = append(wd_rs, wd_oROM[i])
  }
}

for (j in 1:length(wd_rs)){
  wd_rs[[j]] = wd_rs[[j]][-c(2:4)]
}

# get R
wd_r <- list()
for(i in seq(from = 1, to =  length(wd_oROM), by = 3)){
    wd_r = append(wd_r, wd_oROM[i])
}

# get plot data for wod oROM
wod_oROM <- list()
for(i in 1: length(big_table)){
  if (str_detect(names(big_table[i]), "_wod_")){
    wod_oROM = append(wod_oROM, big_table[i])
  }
}

# get VRS
wod_vrs <- list()
for(i in 1: length(wod_oROM)){
  if (str_detect(names(wod_oROM[i]), "_vrs")){
    wod_vrs = append(wod_vrs, wod_oROM[i])
  }
}

for (j in 1:length(wod_vrs)){
  wod_vrs[[j]] = wod_vrs[[j]][-c(2:16)]
}

# get RS
wod_rs <- list()
for(i in seq(from = 2, to =  length(wod_oROM), by = 3)){
    wod_rs = append(wod_rs, wod_oROM[i])
}

for (j in 1:length(wod_rs)){
  wod_rs[[j]] = wod_rs[[j]][-c(2:4)]
}

# get R
wod_r <- list()
for(i in seq(from = 1, to =  length(wod_oROM), by = 3)){
    wod_r = append(wod_r, wod_oROM[i])
}

# create a vector of names
IV_names <- c(
  "IV_8", "IV_9", "IV_10", "IV_11", "IV_12", "IV_13", "IV_14", "IV_15", "IV_16", "IV_17", "IV_18", "IV_19"
)

Rib_names <- c(
  "C8", "C9", "C10", "C11", "C12", "C13", "C14", "C15", "C16", "C17", "C18",
  "DIR10", "DIR11", "DIR12", "DIR13", "DIR14", "DIR15", "DIR16",
  "VIR10", "VIR11", "VIR12", "VIR13", "VIR14", "VIR15", "VIR16"
)

vrs_list <- list(rep("_vrs", 25))
rs_list <- list(rep("_rs", 25))
r_list <- list(rep("_r", 25))
seg_names <- list(vrs_list,
                  rs_list,
                  r_list)



# generate total bar plots to compare oROMs
# IV joints
IV_bar_data <- data.frame(
  jnt = c("IV_8", "IV_9", "IV_10", "IV_11", "IV_12", "IV_13", "IV_14", "IV_15", "IV_16", "IV_17", "IV_18", "IV_19"),
  oROM_max_x = c(IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P8)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P8")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P9)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P9")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P10)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P10")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P11)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P11")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P12)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P12")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P13)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P13")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P14)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P14")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P15)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P15")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P16)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P16")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P17)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P17")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P18)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P18")],
                 IV_jnts[[2]][which.max(abs(IV_jnts$AB_u_max_vrs$flexion_extension_P19)), which(colnames(IV_jnts[[2]]) == "flexion_extension_P19")]),
  
  oROM_wd_x = c(IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P8)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P8")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P9)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P9")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P10)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P10")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P11)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P11")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P12)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P12")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P13)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P13")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P14)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P14")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P15)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P15")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P16)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P16")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P17)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P17")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P18)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P18")],
                 IV_jnts[[4]][which.max(abs(IV_jnts$AB_wd_u_real_vrs$flexion_extension_P19)), which(colnames(IV_jnts[[4]]) == "flexion_extension_P19")]),
 
  oROM_wod_x = c(IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P8)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P8")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P9)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P9")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P10)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P10")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P11)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P11")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P12)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P12")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P13)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P13")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P14)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P14")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P15)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P15")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P16)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P16")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P17)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P17")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P18)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P18")],
                IV_jnts[[6]][which.max(abs(IV_jnts$AB_wod_u_real_vrs$flexion_extension_P19)), which(colnames(IV_jnts[[6]]) == "flexion_extension_P19")]) 
)

# make bar plots
iv_bar <- ggplot() +
  geom_bar(data = IV_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_x), stat = "identity", fill = "#FF4C00", alpha = .3) +
  geom_text(data = IV_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_x, label = oROM_max_x), vjust = ifelse(IV_bar_data$oROM_max_x > 0, 1, -1), size = 3, colour = "black") +
  geom_bar(data = IV_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_x), stat = "identity", fill = "#FF4C00", alpha = .6) +
  geom_text(data = IV_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_x, label = oROM_wod_x), vjust = ifelse(IV_bar_data$oROM_wod_x > 0, 1, -0.5), size = 3, colour = "black") +
  geom_bar(data = IV_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x), stat = "identity", fill = "#FF4C00", alpha = .8) +
  geom_text(data = IV_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x, label = oROM_wd_x), vjust = ifelse(IV_bar_data$oROM_wd_x > 0, 1, -0.5), size = 3, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Intervertebral joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 10),
        axis.title = element_text(size = 12))

ggsave(plot = iv_bar, dpi = 600, bg = "white", 
       filename = "croc_IV_jnt_oROM.png", 
       device = "png", width = 28, heigh = 22, units = "cm")


# vertebral rib joints
# AB setup
# vrs version
cj_vrs_AB_bar_data <- data.frame(
  jnt = c("C8", "C9", "C10", "C11", "C12", "C13", "C14", "C15", "C16", "C17", "C18"),
  oROM_max_x = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C8)), which(colnames(max_vrs[[2]]) == "axial_rotation_C8")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C9)), which(colnames(max_vrs[[2]]) == "axial_rotation_C9")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C10)), which(colnames(max_vrs[[2]]) == "axial_rotation_C10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C11)), which(colnames(max_vrs[[2]]) == "axial_rotation_C11")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C12)), which(colnames(max_vrs[[2]]) == "axial_rotation_C12")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C13)), which(colnames(max_vrs[[2]]) == "axial_rotation_C13")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C14)), which(colnames(max_vrs[[2]]) == "axial_rotation_C14")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C15)), which(colnames(max_vrs[[2]]) == "axial_rotation_C15")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C16)), which(colnames(max_vrs[[2]]) == "axial_rotation_C16")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C17)), which(colnames(max_vrs[[2]]) == "axial_rotation_C17")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_C18)), which(colnames(max_vrs[[2]]) == "axial_rotation_C18")]),


  oROM_max_y = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C8)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C8")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C9)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C9")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C10)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C11)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C11")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C12)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C12")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C13)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C13")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C14)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C14")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C15)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C15")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C16)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C16")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C17)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C17")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_C18)), which(colnames(max_vrs[[2]]) == "abduction_adduction_C18")]),
  
  oROM_max_z = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C8)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C8")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C9)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C9")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C10)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C11)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C11")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C12)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C12")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C13)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C13")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C14)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C14")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C15)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C15")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C16)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C16")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C17)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C17")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$protraction_retraction_C18)), which(colnames(max_vrs[[2]]) == "protraction_retraction_C18")]),

  oROM_wd_x = c(wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C8)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C8")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C9)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C9")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C10)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C10")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C11)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C11")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C12)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C12")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C13)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C13")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C14)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C14")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C15)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C15")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C16)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C16")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C17)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C17")],
                 wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$axial_rotation_C18)), which(colnames(wd_vrs[[2]]) == "axial_rotation_C18")]),
  
  oROM_wd_y = c(wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$abduction_adduction_C8)), which(colnames(wd_vrs[[2]]) == "abduction_adduction_C8")],
                wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$abduction_adduction_C9)), which(colnames(wd_vrs[[2]]) == "abduction_adduction_C9")],
                wd_vrs[[2]][which.max(abs(wd_vrs$AB_wd_u_real_vrs$abduction_adduction_C10)), which(colnames(wd_vrs[[2]]) == "abduction_adduction_C10")],

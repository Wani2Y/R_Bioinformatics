  geom_text(data = cj_rs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_y, label = oROM_wd_y), vjust = ifelse(cj_rs_CBP_bar_data$oROM_wd_y > 0, 1, -1), size = 1, colour = "black") +

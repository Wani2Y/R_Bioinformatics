  geom_bar(data = dij_rs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z), stat = "identity", fill = "#041E42", alpha = .3) +
  geom_text(data = dij_rs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z, label = oROM_max_z), vjust = ifelse(dij_rs_CBP_bar_data$oROM_max_z > 0, 1, -1), size = 1, colour = "black") +

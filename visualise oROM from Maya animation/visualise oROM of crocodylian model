z_AJ_bar <- ggplot() +
  geom_bar(data = cj_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z), stat = "identity", fill = "#E7B10A", alpha = .3) +
  geom_text(data = cj_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z, label = oROM_max_z), vjust = ifelse(cj_r_AJ_bar_data$oROM_max_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z), stat = "identity", fill = "#E7B10A", alpha = .6) +
  geom_text(data = cj_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z, label = oROM_wod_z), vjust = ifelse(cj_r_AJ_bar_data$oROM_wod_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#E7B10A", alpha = .8) +
  geom_text(data = cj_r_AJ_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(cj_r_AJ_bar_data$oROM_wd_z > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))

z_CBP_bar <- ggplot() +
  geom_bar(data = cj_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z), stat = "identity", fill = "#041E42", alpha = .3) +
  geom_text(data = cj_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_max_z, label = oROM_max_z), vjust = ifelse(cj_r_CBP_bar_data$oROM_max_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z), stat = "identity", fill = "#041E42", alpha = .6) +
  geom_text(data = cj_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wod_z, label = oROM_wod_z), vjust = ifelse(cj_r_CBP_bar_data$oROM_wod_z > 0, 1, -1), size = 1, colour = "black") +
  geom_bar(data = cj_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#041E42", alpha = .8) +
  geom_text(data = cj_r_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(cj_r_CBP_bar_data$oROM_wd_z > 0, 1, -1), size = 1, colour = "black") +
  geom_hline(yintercept = 0, linetype = "solid", color = "black", size = 1) + 
  xlab("Costal joints") +
  ylab("Maximal joint rotations (deg)") +
  theme_classic() +
  theme(axis.line.x = element_blank(),
        axis.ticks.x = element_blank(),
        axis.line.y = element_line(colour = "black", linewidth = 0.5, linetype = "solid"),
        axis.text = element_text(size = 8),
        axis.title = element_text(size = 9))

p <- ggarrange(x_AB_bar, x_AJ_bar, x_CBP_bar,
               y_AB_bar, y_AJ_bar, y_CBP_bar,
               z_AB_bar, z_AJ_bar, z_CBP_bar,
               ncol = 3, nrow = 3,
               labels = c("A", "B", "C",
                          "D", "E", "F",
                          "G", "H", "I"),
               common.legend = TRUE, legend = "bottom")

ggsave(plot = p, dpi = 600, bg = "white", 
       filename = "croc_cos_r_bar.png", 
       device = "png", width = 22, heigh = 28, units = "cm")



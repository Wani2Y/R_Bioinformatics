  geom_bar(data = dij_rs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z), stat = "identity", fill = "#041E42", alpha = .8) +
  geom_text(data = dij_rs_CBP_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_z, label = oROM_wd_z), vjust = ifelse(dij_rs_CBP_bar_data$oROM_wd_z > 0, 1, -1), size = 1, colour = "black") +

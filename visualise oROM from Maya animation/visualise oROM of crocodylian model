                 max_rs[[6]][which.max(abs(max_rs$CBP_u_max_rs$protraction_retraction_SR14)), which(colnames(max_rs[[6]]) == "protraction_retraction_SR14")],
                 max_rs[[6]][which.max(abs(max_rs$CBP_u_max_rs$protraction_retraction_SR15)), which(colnames(max_rs[[6]]) == "protraction_retraction_SR15")],
                 max_rs[[6]][which.max(abs(max_rs$CBP_u_max_rs$protraction_retraction_SR16)), which(colnames(max_rs[[6]]) == "protraction_retraction_SR16")]),
  
  oROM_wd_x = c(wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR10)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR10")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR11)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR11")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR12)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR12")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR13)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR13")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR14)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR14")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR15)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR15")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$axial_rotation_SR16)), which(colnames(wd_rs[[6]]) == "axial_rotation_SR16")]),
  
  oROM_wd_y = c(wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$abduction_adduction_SR10)), which(colnames(wd_rs[[6]]) == "abduction_adduction_SR10")],
                wd_rs[[6]][which.max(abs(wd_rs$CBP_wd_u_real_rs$abduction_adduction_SR11)), which(colnames(wd_rs[[6]]) == "abduction_adduction_SR11")],

                 max_r[[6]][which.max(abs(max_r$CBP_u_max_r$axial_rotation_SR16)), which(colnames(max_r[[6]]) == "axial_rotation_SR16")]),
  
  
  oROM_max_y = c(max_r[[6]][which.max(abs(max_r$CBP_u_max_r$abduction_adduction_SR10)), which(colnames(max_r[[6]]) == "abduction_adduction_SR10")],

  oROM_wd_y = c(wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR10)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR10")],
                wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR11)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR11")],
                wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR12)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR12")],
                wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR13)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR13")],
                wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR14)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR14")],
                wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR15)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR15")],
                wd_rs[[2]][which.max(abs(wd_rs$AB_wd_u_real_rs$abduction_adduction_IR16)), which(colnames(wd_rs[[2]]) == "abduction_adduction_IR16")]),
  

  geom_text(data = cj_rs_AB_bar_data, aes(x = factor(jnt, levels = jnt), y = oROM_wd_x, label = oROM_wd_x), vjust = ifelse(cj_rs_AB_bar_data$oROM_wd_x > 0, 1, -1), size = 1, colour = "black") +

# vrs version
vij_vrs_AB_bar_data <- data.frame(
  jnt = c("VIR10", "VIR11", "VIR12", "VIR13", "VIR14", "VIR15", "VIR16"),
  oROM_max_x = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR10)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR11)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR11")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR12)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR12")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR13)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR13")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR14)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR14")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR15)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR15")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$axial_rotation_SR16)), which(colnames(max_vrs[[2]]) == "axial_rotation_SR16")]),
  
  oROM_max_y = c(max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_SR10)), which(colnames(max_vrs[[2]]) == "abduction_adduction_SR10")],
                 max_vrs[[2]][which.max(abs(max_vrs$AB_u_max_vrs$abduction_adduction_SR11)), which(colnames(max_vrs[[2]]) == "abduction_adduction_SR11")],
